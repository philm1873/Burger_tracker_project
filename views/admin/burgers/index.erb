<section>
  <tr>
    <th>Burger Name</th>
    <th>Eatery Name</th>
  </tr>
  <% @burgers.each do |burger| %>
    <tr>
      <td><%= burger.name %></td>
      <td><%= burger.find_eatery.name %></td>
      <td><a href="/admin/burgers/<%= burger.id %>/edit">Edit</a></td>
      <td><a href="/admin/burgers/<%= burger.id %>">Delete</a></td>
    </tr>
  <% end %>
</section>
